{
  "name": "statichunt",
  "description": "statichunt is a static themes, tools, and examples directory",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "yarn update && next dev",
    "build": "yarn update && next build",
    "lint": "next lint",
    "preview": "next start",
    "json": "node scripts/jsonGenerator.mjs",
    "update": "node scripts/jsonGenerator.mjs && node scripts/parseNewThemesData.mjs",
    "update-themes": "yarn json && node scripts/updateThemes.mjs && yarn json",
    "postbuild": "next-sitemap --config next-sitemap.config.mjs",
    "format": "prettier -w ."
  },
  "dependencies": {
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "marked": "^11.0.0",
    "next": "14.0.3",
    "next-mdx-remote": "^4.4.1",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-discord-invite": "^4.0.8",
    "react-dom": "18.2.0",
    "react-gravatar": "^2.6.3",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^4.12.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-use-cookie": "^1.4.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "tailwind-scrollbar": "^3.0.5",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "axios-rate-limit": "^1.3.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.3",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "jshint": "^2.13.6",
    "ora": "^7.0.1",
    "parse-github-url": "^1.0.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "sass": "^1.69.5",
    "tailwind-bootstrap-grid": "^5.1.0",
    "tailwindcss": "^3.3.5",
    "yaml-front-matter": "^4.1.1"
  }
}
