{
  "name": "statichunt",
  "description": "statichunt is a static themes, tools, and examples directory",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "yarn update && next dev",
    "build": "yarn update && node scripts/themeSync.mjs && next build",
    "lint": "next lint",
    "preview": "next start",
    "json": "node scripts/jsonGenerator.mjs",
    "update": "node scripts/jsonGenerator.mjs && node scripts/parseNewThemesData.mjs",
    "update-themes": "yarn json && node scripts/updateThemes.mjs && yarn json",
    "theme-sync": "node scripts/themeSync.mjs",
    "author-sync": "node scripts/authorSync.mjs",
    "postbuild": "next-sitemap --config next-sitemap.config.mjs",
    "format": "prettier -w ."
  },
  "dependencies": {
    "country-list": "^2.3.0",
    "date-fns": "^4.1.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.6",
    "next": "15.1.4",
    "next-mdx-remote": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.4",
    "react": "19.0.0",
    "react-discord-invite": "^4.0.8",
    "react-dom": "19.0.0",
    "react-gravatar": "^2.6.3",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^5.4.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-select": "^5.9.0",
    "react-use-cookie": "^1.6.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "4.0.0",
    "tailwind-scrollbar": "^3.1.0",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "22.10.5",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "axios-rate-limit": "^1.4.0",
    "dotenv": "^16.4.7",
    "eslint": "9.18.0",
    "eslint-config-next": "15.1.4",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "jshint": "^2.13.6",
    "ora": "^8.1.1",
    "parse-github-url": "^1.0.3",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "sass": "^1.83.1",
    "tailwind-bootstrap-grid": "^5.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "yaml-front-matter": "^4.1.1"
  }
}
